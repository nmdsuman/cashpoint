{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/turnament",
      "destination": "/turnament.html"
    },
    {
      "source": "/admin-t",
      "destination": "/turnament.html"
    },
    {
      "source": "/:path((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
